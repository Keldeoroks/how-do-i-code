import math
import numpy as np
import decimal as d

bpm = input("insert bpm: ")
t = input("insert INHERITED timing points (green lines): ")
s = t.split(" ")
timings = np.array(s)
for x in timings:
    arr = x.split(sep=",")
    nt = 1 / d.Decimal(arr[1]) * d.Decimal(-100) * d.Decimal(bpm)
    print("timing(", end = "")
    print(arr[0], end = "")
    print(",", end = "")
    print(round(nt,2), end = "")
    print(",", end = "")
    print(arr[2], end = "")
    print(")")
